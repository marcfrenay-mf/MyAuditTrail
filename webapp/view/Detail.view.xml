<mvc:View controllerName="pcc.statistic.sdwp.controller.Detail" xmlns="sap.uxap" xmlns:m="sap.m" xmlns:f="sap.f"
	xmlns:form="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns:grid="sap.ui.layout.cssgrid">
	<ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true" alwaysShowContentHeader="false" preserveHeaderStateOnScroll="false"
		headerContentPinnable="true" isChildPage="true" upperCaseAnchorBar="false">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle id="IDGenObjectPageDynamicHeaderTitle1">
				<expandedHeading>
					<m:Title id="IDGenTitle1" text="{OverviewModel>name}" wrapping="true" class="sapUiSmallMarginEnd"/>
				</expandedHeading>
				<snappedHeading>
					<m:FlexBox id="IDGenFlexBox1" wrap="Wrap" fitContainer="true" alignItems="Center">
						<m:FlexBox id="IDGenFlexBox2" wrap="NoWrap" fitContainer="true" alignItems="Center" class="sapUiTinyMarginEnd">
							<f:Avatar id="IDGenAvatar1" src="https://sdk.openui5.org/test-resources/sap/ui/documentation/sdk/images/HT-1000.jpg" displaySize="S" displayShape="Square"
								showBorder="true" class="sapUiTinyMarginEnd"/>
							<m:Title id="IDGenTitle2" text="{OverviewModel>name}" wrapping="true"/>
						</m:FlexBox>
					</m:FlexBox>
				</snappedHeading>
				<actions>
					<!--<m:ToggleButton text="Edit" type="Emphasized" press=".onEditToggleButtonPress"/>-->
					<!--<m:Button text="Delete" type="Transparent"/>-->
					<!--<m:Button text="Copy" type="Transparent"/>-->
					<m:Button id="IDGenButton1" icon="sap-icon://action" type="Transparent" press=".onActionPress"/>
				</actions>
				<navigationActions>
					<m:OverflowToolbarButton id="IDGenOverflowToolbarButton1" type="Transparent" icon="sap-icon://full-screen" press=".handleFullScreen" tooltip="Enter Full Screen Mode"
						visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }"/>
					<m:OverflowToolbarButton id="IDGenOverflowToolbarButton2" type="Transparent" icon="sap-icon://exit-full-screen" press=".handleExitFullScreen" tooltip="Exit Full Screen Mode"
						visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }"/>
					<m:OverflowToolbarButton id="IDGenOverflowToolbarButton3" type="Transparent" icon="sap-icon://decline" press=".handleClose" tooltip="Close column"
						visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"/>
				</navigationActions>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<m:FlexBox id="IDGenFlexBox3" wrap="Wrap" fitContainer="true" alignItems="Stretch">
				<f:Avatar id="IDGenAvatar2" src="https://sdk.openui5.org/test-resources/sap/ui/documentation/sdk/images/HT-1000.jpg" displaySize="L" displayShape="Square" class="sapUiTinyMarginEnd"></f:Avatar>
				<m:VBox id="IDGenVBox1" justifyContent="Center" class="sapUiSmallMarginEnd">
					<m:Label id="IDGenLabel1" text="{i18n>ProcessType}"/>
					<m:Text id="IDGenText1" text="{OverviewModel>Type}"/>
				</m:VBox>
				<m:VBox id="IDGenVBox2" justifyContent="Center" class="sapUiSmallMarginEnd">
					<m:Label id="IDGenLabel2" text="{i18n>CreatedBy}"/>
					<m:Text id="IDGenText2" text="{OverviewModel>CreatedBy}"/>
				</m:VBox>
				<m:VBox id="IDGenVBox3" justifyContent="Center" class="sapUiSmallMarginEnd">
					<m:Label id="IDGenLabel3" text="{i18n>LastChangedBy}"/>
					<m:Text id="IDGenText3" text="{OverviewModel>LastChangedBy} "/>
				</m:VBox>
				<m:VBox id="IDGenVBox4" justifyContent="Center" class="sapUiSmallMarginEnd">
					<m:Label id="IDGenLabel8" text="{i18n>Status}"/>
					<m:Text id="IDGenText8" text="{OverviewModel>Status} "/>
				</m:VBox>
			</m:FlexBox>
		</headerContent>
		<sections>
			<ObjectPageSection id="IDGenObjectPageSection1" title="{i18n>Tab1}" titleUppercase="false" >
				<subSections>
					<ObjectPageSubSection id="IDGenObjectPageSubSection1">
						<blocks>
							<f:GridList id="gridList" mode="SingleSelectMaster" items="{ path: 'LocalModel>/KPI' }"	selectionChange="onKPIPress" class="sapUiResponsiveContentPadding">
								<f:customLayout>
									<grid:GridBasicLayout id="_IDGenGridBasicLayout" gridTemplateColumns="repeat(auto-fit, minmax(16rem, 1fr))" gridGap="0.5rem" />
								</f:customLayout>

								<f:GridListItem id="_IDGenGridListItem"	counter="{LocalModel>Quantity}" highlight="{LocalModel>Status}" >
									<m:VBox id="_IDGenVBox1" height="100%">				
										<m:HBox id="_IDGenHBox" height="100%" class="sapUiSmallMargin" alignItems="Center">					
											<m:VBox id="_IDGenVBox2">
												<m:Title id="_IDGenTitle" text="{LocalModel>Name}" wrapping="true" />
												<m:Label id="_IDGenLabel" text="{LocalModel>Description}" wrapping="true" />
											</m:VBox>
										</m:HBox>
									</m:VBox>
								</f:GridListItem>
							</f:GridList>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="IDGenObjectPageSection2" title="{i18n>Tab2}" titleUppercase="false">
				<subSections>
					<ObjectPageSubSection id="IDGenObjectPageSubSection2">
						<blocks>
							<m:Table id="validationRuleTable" items="{path : 'OverviewModel>/ValidationRule'}" growing="true" growingThreshold="15" busy="{LocalModel>/IsDataLoading}">
								<m:columns>
									<m:Column id="IDGenColumn1"/>
								</m:columns>
								<m:items>
									<m:ColumnListItem id="IDGenColumnListItem1" type="Navigation" press=".onValidationRulePress">
										<m:cells>
											<m:ObjectIdentifier id="IDGenObjectIdentifier1" text="{OverviewModel>Name}"/>
										</m:cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
		<footer>
			<m:OverflowToolbar id="IDGenOverflowToolbar1">
				<m:ToolbarSpacer id="IDGenToolbarSpacer1"/>
				<m:Button id="IDGenButton2" type="Accept" text="Save"/>
				<m:Button id="IDGenButton3" type="Reject" text="Cancel"/>
			</m:OverflowToolbar>
		</footer>
	</ObjectPageLayout>
</mvc:View>